{
  "name": "eftec/chaosmachineone",
  "description": "A Chaos Machine for PHP",
  "type": "library",
  "keywords": [
    "php",
    "chaosmachine",
    "database"
  ],
  "homepage": "https://github.com/EFTEC/ChaosMachineOne",
  "authors": [
    {
      "name": "Jorge Castro Castillo",
      "homepage": "https://github.com/EFTEC/"
    }
  ],
  "minimum-stability": "beta",
  "license": "LGPL-3.0",
  "require": {
    "php": ">=7.2",
    "composer/installers": "^2.2.0",
    "eftec/minilang": "^2.27",
    "ext-ctype": "*",
    "eftec/pdoone": "3.12.2",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "eftec\\": "vendor/eftec/",
      "eftec\\chaosmachineone\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "eftec\\tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
